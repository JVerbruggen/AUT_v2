func test{
    assert 1 == 1
    assert -2^2 == -4
    assert (2+2)*2 == 8

    assert 3! == 6
    assert 4! == 24

    assert false == false
    assert true == true

    assert 3-2 == 0 == false
    assert 3-2 == 1 == true

    assert 1 < 2 == true
    assert 1 < 1 == false
    assert 1 < 0 == false

    assert 2 <= 4 == true
    assert 2 <= 3 == true
    assert 2 <= 2 == true
    assert 2 <= 1 == false

    assert 2 > 4 == false
    assert 2 > 3 == false
    assert 2 > 2 == false
    assert 2 > 1 == true

    assert 2 >= 4 == false
    assert 2 >= 3 == false
    assert 2 >= 2 == true
    assert 2 >= 1 == true

    assert 2 != 3 == true
    assert 2 != 2 == false
